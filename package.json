{
  "name": "vite-vue-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.vue\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "-": "^0.0.1",
    "D": "^1.0.0",
    "autoprefixer": "^10.2.5",
    "element-plus": "^1.0.2-beta.33",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.3",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0",
    "vue-router": "4",
    "vuedraggable": "^4.0.1",
    "vuex": "^4.0.0",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.32",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "typescript": "^4.1.3",
    "vite": "^2.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,json,css,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
